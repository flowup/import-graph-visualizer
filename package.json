{
  "name": "import-graph-visualizer",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --open --hot --mode development",
    "build": "npm run build:app && npm run build:cli",
    "build:app": "webpack --mode production",
    "build:cli": "tsc src/cli/index.ts --outDir dist/cli",
    "build:wasm": "em++ src/wasm/cpp/graph.cpp -s MODULARIZE -s ALLOW_MEMORY_GROWTH=1 -s EXPORTED_FUNCTIONS=_findAllPaths,_malloc,_free -s EXTRA_EXPORTED_RUNTIME_METHODS=cwrap,stringToUTF8,lengthBytesUTF8 -O3 -o src/wasm/generated/graph.js",
    "cli": "npm run build:cli && node dist/cli/index.js",
    "fetch-icons": "ts-node scripts/fetch-icons.ts"
  },
